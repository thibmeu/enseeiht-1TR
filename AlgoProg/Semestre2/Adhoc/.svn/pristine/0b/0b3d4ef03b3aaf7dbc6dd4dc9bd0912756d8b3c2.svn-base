public class EnvoiePaquet extends Operation {
	
	private int dateFinEmission;
	private Noeud noeud;
	
	public EnvoiePaquet(int temps, Simulateur simulateur, Noeud noeud){
		super();
		dateFinEmission = temps + simulateur.getDateCourante();
		this.noeud = noeud;
	}
	
	public void executer(Simulateur simulateur, int date) {
		noeud.supprimerBuff();
	}
		
		
		
		
		
	}
